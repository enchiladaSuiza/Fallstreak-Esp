    # ido "THAT’S MY LITTLE SONGBIRD FOR YOU!!" with Shake((0, 0, 0, 0), 0.5, dist=10)
    # TODO cambiar
    ido "¡¡ESA ES MI CANARITA!!" with Shake((0, 0, 0, 0), 0.5, dist=10)

# game/script.rpy:11395
