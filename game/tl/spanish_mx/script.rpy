    # tr2 "..."
    tr2 "..."

# game/script.rpy:12108
translate spanish_mx start_c29df275:

    # narrator2 "At my side Tris peeks over to where Callia is resting."
    narrator2 "A mi lado, Tris se asoma hacia donde Callia reposa."

# game/script.rpy:12109
translate spanish_mx start_75c51d39:

    # narrator2 "He looks back and forth between Callia’s absent arms and his own intact ones."
    narrator2 "Echa miradas entre los brazos ausentes de Callia y los suyos intactos."

# game/script.rpy:12110
translate spanish_mx start_6f75d4d5:

    # au "Guh…{w=0.35} agh!"
    au "Guh…{w=0.35} ¡agh!"

# game/script.rpy:12111
translate spanish_mx start_3c4a367d:

    # narrator2 "A bead of sweat rolls down Autune’s face as he writhes in pain."
    narrator2 "Una gota de sudor recorre el rostro de Autune mientras se retuerce de dolor."

# game/script.rpy:12112
translate spanish_mx start_f9b19449:

    # am "It’s spreading again…"
    am "Se está esparciendo otra vez…"

# game/script.rpy:12113
translate spanish_mx start_e85bf36d:

    # narrator2 "A bizarre phenomenon plays out before me."
    narrator2 "Un fenómeno extraño ocurre frente a mí."

# game/script.rpy:12114
translate spanish_mx start_17669652:

    # narrator2 "Without warning, the marked areas on Autune’s hands begin to glow."
    narrator2 "Sin advertencia, las áreas marcadas en las manos de Autune comienzan a brillar."

# game/script.rpy:12115
translate spanish_mx start_0206b934:

    # narrator2 "When the light subsides I observe that the burn patterns have crept up his arms."
    narrator2 "Cuando la luz desaparece, observo que los patrones de las quemaduras habían subido sobre sus muñecas."

# game/script.rpy:12116
translate spanish_mx start_bd7d295b:

    # narrator2 "The same thing was happening to Callia’s burns as well."
    narrator2 "Lo mismo pasaba con las quemaduras de Callia."

# game/script.rpy:12117
translate spanish_mx start_fe77db13:

    # narrator2 "It seemed the wounds inflicted by the unusual fire were harmful even after their initial infliction."
    narrator2 "Parecía que las heridas causadas por el fuego inusual eran dañinas incluso después del primer contacto."

# game/script.rpy:12118
translate spanish_mx start_fc16e103:

    # narrator2 "It was like a disease…{w=0.4} or a curse."
    narrator2 "Era como una enfermedad…{w=0.4} o una maldición."

# game/script.rpy:12119
translate spanish_mx start_60b5afc1:

    # narrator2 "I had to find a way to treat it before it did any more damage."
    narrator2 "Necesitaba encontrar la manera de tratarlo antes de que hiciera más daño."

# game/script.rpy:12120
translate spanish_mx start_fdb7616b:

    # am "Watch over daddy and Callia, Tris."
    am "Cuida de papi y de Callia, Tris."

# game/script.rpy:12121
translate spanish_mx start_4389aabc:

    # narrator2 "Food, water, and medical supplies had already been set down in arm’s reach of the bed."
    narrator2 "Comida, agua y suministros médicos ya habían sido colocados al alcance de la mano cerca de la cama."

# game/script.rpy:12122
translate spanish_mx start_31876721:

    # am "Don’t leave.\nDon’t let anyone in."
    am "No salgas.\nNo dejes entrar a nadie."

# game/script.rpy:12126
translate spanish_mx start_0156beac:

    # narrator2 "Retreating into my room, I make preparations to set off."
    narrator2 "Regresando a mi habitación, hago los preparativos para salir."

# game/script.rpy:12127
translate spanish_mx start_37819a72:

    # narrator2 "A locked drawer."
    narrator2 "Un cajón cerrado con llave."

# game/script.rpy:12128
translate spanish_mx start_78ace5ff:

    # narrator2 "For years I had only ever touched it to maintain its contents."
    narrator2 "Por años, solo lo he tocado para dar mantenimiento a sus contenidos."

# game/script.rpy:12129
translate spanish_mx start_3fbd740e_1:

    # narrator2 "{i}My heart beats not.{/i}"
    narrator2 "{i}Mi corazón no palpita.{/i}"

# game/script.rpy:12130
translate spanish_mx start_f9f88913:

    # narrator2 "After inspecting them one by one I tighten a holster of throwing knives around my leg."
    narrator2 "Tras inspeccionarlos uno por uno, amarro una funda de cuchillos arrojadizos alrededor de mi pierna."

# game/script.rpy:12131
translate spanish_mx start_9424ba9b:

    # narrator2 "As I do, my eyes drift over to the proof of my existence— a razor sharp butcher knife that curved to a tapered point."
    narrator2 "Mientras lo hago, mis ojos voltean hacia la prueba de mi existencia— un afilado cuchillo de carnicero que se curvaba a una punta cónica." 

# game/script.rpy:12132
translate spanish_mx start_1c419376_1:

    # narrator2 "{i}Breath does not come from my mouth.{/i}"
    narrator2 "{i}El aliento no sale de mi boca.{/i}"

# game/script.rpy:12133
translate spanish_mx start_963bd49e:

    # narrator2 "The scratches and chips that marred it were proof I had succeeded in carving a path to this day."
    narrator2 "Los rasguños y desprendimientos que lo mancillaban eran prueba de que había tenido éxito en labrar un camino para este día."

# game/script.rpy:12134
translate spanish_mx start_164c86a4:

    # narrator2 "Even after all these years it still felt natural in my hand, as if it were but an extension of my body."
    narrator2 "Incluso después de todos estos años todavía se sentía natural en mi mano, como si no fuera más que una extensión de mi cuerpo."

# game/script.rpy:12135
translate spanish_mx start_47e8b8f6_1:

    # narrator2 "{i}My feet do not tread upon this earth.{/i}"
    narrator2 "{i}Mis pies no pisan sobre esta tierra.{/i}"

# game/script.rpy:12136
translate spanish_mx start_69aadaa0_1:

    # narrator2 "{i}For I am colorless.{/i}"
    narrator2 "{i}Puesto que soy incolora.{/i}"

# game/script.rpy:12137
translate spanish_mx start_8e4882c4:

    # narrator2 "The weight of steel was one of the only absolute truths in life."
    narrator2 "El peso del acero era una de las pocas verdades absolutas de la vida."

# game/script.rpy:12138
translate spanish_mx start_4806e9af:

    # narrator2 "Sincere and free of lies— it was that quality of purity that enabled it to spill blood with ease."
    narrator2 "Sincero y libre de mentiras— era esa calidad de pureza que le permitía salpicar sangre con facilidad."

# game/script.rpy:12139
translate spanish_mx start_ce1c3bac:

    # am "The past was never something I could escape, was it?"
    am "El pasado nunca fue algo de lo que pudiera escapar, ¿o sí?"

# game/script.rpy:12140
translate spanish_mx start_66f379c1:

    # narrator2 "Concealing the knife on my person, I lock the door behind me as I leave."
    narrator2 "Escondiendo el cuchillo en mi persona, cierro la puerta detrás de mí al tiempo que me voy."

# game/script.rpy:12151
translate spanish_mx start_71b17b6e:

    # narrator2 "In contrast to yesterday’s chaos, a somber silence had laid claim to the hazy morning air."
    narrator2 "En contraste con el caos de ayer, un sombrío silencio se había apoderado del nubloso aire de la mañana."

# game/script.rpy:12152
translate spanish_mx start_d7a0a6f0:

    # narrator2 "The situation had changed since yesterday."
    narrator2 "La situación había cambiado desde ayer."

# game/script.rpy:12153
translate spanish_mx start_81674dc9:

    # narrator2 "At some point the inextinguishable fire had dissipated all at once, ending just as abruptly as it had begun."
    narrator2 "En algún punto el fuego inextinguible se había disipado por completo en un instante, terminando tan abruptamente como empezó."

# game/script.rpy:12154
translate spanish_mx start_2c0a5130:

    # narrator2 "Only the smoldering remains it left behind could be seen now."
    narrator2 "Ahora solo eran visibles los restos humeantes que dejó atrás."

# game/script.rpy:12155
translate spanish_mx start_d7e62b3b:

    # am "I’ll check up on Mallory first."
    am "Primero iré a ver a Mallory."

# game/script.rpy:12159
translate spanish_mx start_662ac6ce:

    # narrator2 "Though it takes longer than usual to get to the Bacia household with all the debris, I arrive without complication."
    narrator2 "Aunque me toma más tiempo de lo usual llegar a la casa de los Bacia con todo el escombro, lo consigo sin complicaciones."

# game/script.rpy:12160
translate spanish_mx start_0d17e5af:

    # narrator2 "While their house appeared to be untouched, many of their neighbors had been less fortunate."
    narrator2 "Mientras que su casa parecía estar intacta, muchos de sus vecinos habían sido menos afortunados."

# game/script.rpy:12161
translate spanish_mx start_7ae9bdca:

    # narrator2 "I find Mallory in the streets kneeling over a victim she seemed to have pulled from the rubble."
    narrator2 "Encuentro a Mallory en las calles arrodillada sobre una víctima que parecía haber sacado de los escombros."

# game/script.rpy:12162
translate spanish_mx start_4a7fe138_5:

    # am "Mallory."
    am "Mallory."

# game/script.rpy:12163
translate spanish_mx start_83d0570a:

    # narrator2 "Though I call out to her, she doesn’t respond."
    narrator2 "Aunque la llamo, no responde."

# game/script.rpy:12164
translate spanish_mx start_17099438:

    # narrator2 "It is only when I place a hand on her shoulder that she turns to face me."
    narrator2 "Solo es cuando coloco una mano en su hombro que voltea a verme."

# game/script.rpy:12165
translate spanish_mx start_235b1597_2:

    # ma "Amie…"
    ma "Amie…"

# game/script.rpy:12166
translate spanish_mx start_1c973ae2:

    # narrator2 "Glazed hollow eyes that gazed through what they were supposed to focus on."
    narrator2 "Ojos vidriosos y vacíos que veían a través de lo que tenían que enfocarse."

# game/script.rpy:12167
translate spanish_mx start_41b161a0:

    # narrator2 "Every person I had passed on the way had looked the same."
    narrator2 "Todas las personas por las que pasé en el camino se veían igual."

# game/script.rpy:12168
translate spanish_mx start_ae3c2ddf:

    # ma "The fire.{w=0.2} Idone, burn marks…{w=0.15} Socotrine…{w=0.1} Dia—{w=0.15} I…"
    ma "El fuego.{w=0.2} Idone, quemaduras…{w=0.15} Socotrine…{w=0.1} Dia—{w=0.15} Yo…"

# game/script.rpy:12169
translate spanish_mx start_d4389c81:

    # am "Where is Idone?{w=0.25} Did he survive?"
    am "¿Dónde está Idone?{w=0.25} ¿Sobrevivió?"

# game/script.rpy:12170
translate spanish_mx start_0b088978:

    # narrator2 "I pull her together by giving her something specific to focus on."
    narrator2 "Intento tranquilizarla dándole algo específico en lo que concentrarse."

# game/script.rpy:12171
translate spanish_mx start_00415233:

    # narrator2 "But Mallory shakes her head."
    narrator2 "Pero Mallory sacude su cabeza."

# game/script.rpy:12172
translate spanish_mx start_71bbf26a:

    # ma "One of his colleagues came by…{w=0.2} and…"
    ma "Uno de sus compañeros vino…{w=0.2} y…"

# game/script.rpy:12173
translate spanish_mx start_1b577866:

    # ma "I don’t know what to tell the children…"
    ma "No sé qué decirle a los niños…"

# game/script.rpy:12174
translate spanish_mx start_cf257506:

    # am "There’s no need to tell them now.\nWorry about that later."
    am "No hay necesidad de decirles ahora.\nPreocúpate de eso después."

# game/script.rpy:12175
translate spanish_mx start_2ec3e4cb:

    # am "How are the children’s conditions?"
    am "¿Cómo están los niños?"

# game/script.rpy:12176
translate spanish_mx start_a3809710:

    # ma "They’re inside.{w=0.25} Alouette hurt her leg, but other than that she’s okay."
    ma "Están adentro.{w=0.25} Alouette se lesionó la pierna, pero fuera de eso está bien."

# game/script.rpy:12177
translate spanish_mx start_9bf57945:

    # ma "Linette and Dia though…"
    ma "Pero Linette y Dia…"

# game/script.rpy:12178
translate spanish_mx start_6e519dca:

    # ma "I-{w=0.15}I’ll show you after I finish treating this man."
    ma "Te-{w=0.15} te mostraré cuando termine de tratar a este hombre."

# game/script.rpy:12179
translate spanish_mx start_84639cd6:

    # narrator2 "Though her hands were trembling, she doesn’t stop bandaging the wounded man before her."
    narrator2 "Aunque sus manos tiemblan, no para de vendar al hombre herido frente a ella."

# game/script.rpy:12180
translate spanish_mx start_1026f885:

    # narrator2 "It was like a desperate attempt to remain moving— as if she’d collapse under the pressure if she stopped."
    narrator2 "Era como un intento desesperado de seguir moviéndose— como si fuera a colapsar bajo la presión si se detuviera."

# game/script.rpy:12181
translate spanish_mx start_5b8d374e:

    # narrator2 "It was a way of averting one’s eyes from reality."
    narrator2 "Era una manera de apartar los ojos de la realidad."

# game/script.rpy:12182
translate spanish_mx start_73ee62ca:

    # am "Forget it."
    am "Olvídalo."

# game/script.rpy:12183
translate spanish_mx start_3307c1d9:

    # am "Give up on him."
    am "Ríndete con él."

# game/script.rpy:12184
translate spanish_mx start_8d9cfd8a:

    # am "If you insist on trying to help, conserve your energy for someone who has a chance of making it."
    am "Si insistes en ayudar, conserva tu energía para alguien que tenga una oportunidad de sobrevivir."

# game/script.rpy:12185
translate spanish_mx start_d73b06c6:

    # narrator2 "A ragged avulsion splitting the femoral."
    narrator2 "Una avulsión precaria que separaba el fémur."

# game/script.rpy:12186
translate spanish_mx start_dadf8d25:

    # narrator2 "Limbs entangled at impossible angles."
    narrator2 "Extremidades enredadas en ángulos imposibles."

# game/script.rpy:12187
translate spanish_mx start_7bb74ad4:

    # narrator2 "And copious amounts of blood."
    narrator2 "Y abundantes cantidades de sangre."

# game/script.rpy:12188
translate spanish_mx start_55c25b35:

    # narrator2 "It was clear from a glance the man was already at death’s door."
    narrator2 "Era claro a simple vista que el hombre ya estaba frente a la puerta de la muerte."

# game/script.rpy:12189
translate spanish_mx start_47719771:

    # narrator2 "Mallory hunches over and closes her eyes as her body shivers to a halt."
    narrator2 "Mallory se encorva y cierra sus ojos mientras su cuerpo deja de estremecerse."

# game/script.rpy:12190
translate spanish_mx start_5056c68c:

    # narrator2 "For a moment I worry she’ll restart her mindless act."
    narrator2 "Por un momento me preocupa que resuma su acto insensato."

# game/script.rpy:12191
translate spanish_mx start_b6b8dad2:

    # ma "You’re right."
    ma "Tienes razón."

# game/script.rpy:12192
translate spanish_mx start_4cb01ec0:

    # narrator2 "But then she rises to her feet, determination filling her eyes."
    narrator2 "Pero entonces se pone de pie, sus ojos llenos de determinación."

# game/script.rpy:12193
translate spanish_mx start_d56dbd8b:

    # ma "Let’s head inside."
    ma "Vamos adentro."

# game/script.rpy:12198
translate spanish_mx start_6af56590:

    # narrator2 "A house painted over with silence."
    narrator2 "Una casa pintada de silencio."

# game/script.rpy:12199
translate spanish_mx start_9d6e8e83:

    # narrator2 "It was not long ago that laughter filled these halls."
    narrator2 "No fue hace mucho que las risas llenaban estos pasillos."

# game/script.rpy:12200
translate spanish_mx start_2d65dbf4:

    # narrator2 "Guided by Mallory, I make my way to the room the children are in."
    narrator2 "Guiada por Mallory, me abro paso hacia la habitación donde están los niños."

# game/script.rpy:12204
translate spanish_mx start_7ef0defc:

    # narrator2 "Upon entering, a pitiful scene greets me."
    narrator2 "Al entrar, me recibe una escena lamentable."

# game/script.rpy:12205
translate spanish_mx start_c9838591:

    # narrator2 "Laid out on the bed were Linette and Dia, their bodies disheveled and bandaged— their breaths, short and ragged."
    narrator2 "Acostados sobre la cama estaban Linette y Dia, sus cuerpos desarreglados y vendados— sus respiraciones, cortas e intermitentes."

# game/script.rpy:12206
translate spanish_mx start_3d9a972a:

    # narrator2 "At the other end of the room Alouette was sitting against the wall in a state of stunned silence."
    narrator2 "Al otro lado del cuarto Alouette se sentaba contra la pared en un estado de silencio paralizado."

# game/script.rpy:12207
translate spanish_mx start_ac83e467:

    # narrator2 "Her leg was injured and she didn’t look capable of moving, but fortunately she didn’t appear to be burned anywhere."
    narrator2 "Su pierna estaba herida y no parecía capaz de moverse, pero afortunadamente no parecía tener quemaduras en ninguna parte."

# game/script.rpy:12208
translate spanish_mx start_53c359b4:

    # am "Have you noticed Mallory?"
    am "¿Lo notaste, Mallory?"

# game/script.rpy:12209
translate spanish_mx start_4278ed37:

    # ma "Yeah.{w=0.2} Their burns won’t stop spreading."
    ma "Sí.{w=0.2} Sus quemaduras no dejan de extenderse."

# game/script.rpy:12210
translate spanish_mx start_91f52327:

    # ma "The rainbow fire did this to them."
    ma "El fuego arcoíris les hizo esto."

# game/script.rpy:12211
translate spanish_mx start_64df4997:

    # am "Rainbow fire…"
    am "Fuego arcoíris…"

# game/script.rpy:12212
translate spanish_mx start_778b4ce3:

    # narrator2 "Their burns were even more extensive than Callia and Autune’s."
    narrator2 "Sus quemaduras eran mucho más extensas que las de Callia y Autune."

# game/script.rpy:12213
translate spanish_mx start_f87b7361:

    # narrator2 "Several parts of Dia’s back and face seemed afflicted, while burn marks were scattered over almost a third of Linette’s body."
    narrator2 "Varias partes de la espalda y rostro de Dia estaban afectados, mientras que las quemaduras se esparcían por casi un tercio del cuerpo de Linette."

# game/script.rpy:12214
translate spanish_mx start_f10715b2:

    # am "I’ll find a way to save them."
    am "Encontraré una manera de salvarlos."

# game/script.rpy:12215
translate spanish_mx start_65cba197:

    # narrator2 "Mallory’s eyes widen at my declaration."
    narrator2 "Los ojos de Mallory se agrandan ante mi declaración."

# game/script.rpy:12216
translate spanish_mx start_1826a8d8:

    # ma "How?"
    ma "¿Cómo?"

# game/script.rpy:12217
translate spanish_mx start_a67f1e0f:

    # am "Information is the first thing we need."
    am "Lo primero que necesitamos es información."

# game/script.rpy:12218
translate spanish_mx start_752b0c76:

    # am "I’m going to scope out Aachen Neues."
    am "Iré a investigar el Aachen Neues."

# game/script.rpy:12219
translate spanish_mx start_8f749cef:

    # am "If anyone knows anything about the rainbow fire, it’s the ULs."
    am "Si alguien sabe algo sobre el fuego arcoíris, son los TDs."

# game/script.rpy:12220
translate spanish_mx start_301abcb3:

    # ma "Amie… are you sure?"
    ma "Amie… ¿estás segura?"

# game/script.rpy:12221
translate spanish_mx start_91efb6dc:

    # am "I won’t fail."
    am "No fallaré."

# game/script.rpy:12222
translate spanish_mx start_17ed8177:

    # am "Callia and Autune are in the same situation after all."
    am "Después de todo, Callia y Autune están en la misma situación."

# game/script.rpy:12223
translate spanish_mx start_53af8192:

    # ma "That’s not what I mean."
    ma "No me refiero a eso."

# game/script.rpy:12224
translate spanish_mx start_2fab559c:

    # ma "You’re always like this…"
    ma "Siempre eres así…"

# game/script.rpy:12225
translate spanish_mx start_a6647bea:

    # ma "You always try to bear everything by yourself."
    ma "Siempre intentas soportar todo tú sola."

# game/script.rpy:12226
translate spanish_mx start_5c4f9a75:

    # ma "It worries me."
    ma "Me preocupa."

# game/script.rpy:12227
translate spanish_mx start_1c8acf17_19:

    # am "..."
    am "..."

# game/script.rpy:12228
translate spanish_mx start_bcd0e2e1:

    # ma "I just want you to know you have allies."
    ma "Solo quiero que sepas que tienes aliados."

# game/script.rpy:12229
translate spanish_mx start_66be0342:

    # ma "Lean on me.{w=0.3} Let me help."
    ma "Apóyate en mí.{w=0.3} Déjame ayudar."

# game/script.rpy:12230
translate spanish_mx start_69f24f57:

    # am "No.{w=0.2} You shouldn’t come."
    am "No.{w=0.2} No deberías venir."

# game/script.rpy:12231
translate spanish_mx start_705a76b3:

    # am "It might turn dangerous and you’ll only slow me down."
    am "Puede volverse peligroso y solo me estorbarás."

# game/script.rpy:12232
translate spanish_mx start_235b1597_3:

    # ma "Amie…"
    ma "Amie…"

# game/script.rpy:12233
translate spanish_mx start_b8c808d7:

    # am "You can help by stocking up on supplies."
    am "Puedes ayudar acumulando suministros."

# game/script.rpy:12234
translate spanish_mx start_3c6408a2:

    # am "I also left Autune and the twins at home."
    am "También dejé a Autune y a los gemelos en casa."

# game/script.rpy:12235
translate spanish_mx start_85e56179:

    # am "I don’t know how long I’ll be gone, so I want you to check up on them."
    am "No sé por cuánto tiempo estaré fuera, así que quiero que los cuides."

# game/script.rpy:12236
translate spanish_mx start_9ddc03d3_1:

    # ma "..."
    ma "..."

# game/script.rpy:12237
translate spanish_mx start_16fc2b8a:

    # narrator2 "It still looked like she had something to say though."
    narrator2 "Parecía que aún tenía algo que decir."

# game/script.rpy:12238
translate spanish_mx start_b84a7362:

    # am "I want you to believe in me Mallory."
    am "Quiero que creas en mí, Mallory."

# game/script.rpy:12239
translate spanish_mx start_ebc55a19:

    # am "Trust me."
    am "Confía en mí."

# game/script.rpy:12240
translate spanish_mx start_f25747d2:

    # narrator2 "I clasp her hands."
    narrator2 "La tomo de las manos."

# game/script.rpy:12241
translate spanish_mx start_d2997ac0:

    # ma "Okay…"
    ma "Sí…"

# game/script.rpy:12242
translate spanish_mx start_479c5dd4:

    # ma "Just be careful, alright?"
    ma "Solo ten cuidado, ¿sí?"

# game/script.rpy:12243
translate spanish_mx start_b1c92049:

    # ma "Promise me you won’t do anything too reckless."
    ma "Prométeme que no harás nada demasiado imprudente."

# game/script.rpy:12244
translate spanish_mx start_8abfee47:

    # am "I promise."
    am "Lo prometo."

# game/script.rpy:12245
translate spanish_mx start_ba19b89a:

    # narrator2 "The two of us entwine pinkies like in the old days."
    narrator2 "Las dos entrelazamos meñiques como en los viejos tiempos."

# game/script.rpy:12246
translate spanish_mx start_0434681a_3:

    # ma "Huhu…"
    ma "Juju…"

# game/script.rpy:12247
translate spanish_mx start_7e479ddc:

    # ma "We’ll get through this okay?"
    ma "Saldremos de esto, ¿de acuerdo?"

# game/script.rpy:12248
translate spanish_mx start_8afbe0f1:

    # am "Yeah."
    am "Sí."

# game/script.rpy:12249
translate spanish_mx start_b36c8d7e:

    # am "I’ll be off then."
    am "Entonces me voy."

# game/script.rpy:12250
translate spanish_mx start_ac2060be:

    # narrator2 "As I turn to leave, my eyes catch on Alouette standing in the corner of the room."
    narrator2 "Mientras me volteo para salir, mis ojos captan a Alouette parada en la esquina del cuarto."

# game/script.rpy:12251
translate spanish_mx start_dd71f9dd:

    # narrator2 "Though she said nothing, her face spoke for itself."
    narrator2 "Aunque no decía nada, su expresión hablaba por sí misma."

# game/script.rpy:12252
translate spanish_mx start_ad92ba36:

    # narrator2 "It was as if she were lost in a sea of uncertainty."
    narrator2 "Era como si estuviera perdida en un mar de incertidumbre."

# game/script.rpy:12253
translate spanish_mx start_f5dfb100:

    # narrator2 "Before I know it I find myself drifting over to her."
    narrator2 "Antes de darme cuenta me encuentro acercándome a ella."

# game/script.rpy:12254
translate spanish_mx start_e8492952:

    # narrator2 "Maybe it was because she reminded me of a certain someone, but somehow I felt a desire to not leave her alone."
    narrator2 "Quizás era porque me recordaba a una cierta persona, pero por alguna razón sentía el deseo de no dejarla sola."

# game/script.rpy:12255
translate spanish_mx start_4240e6f7:

    # am "Alouette."
    am "Alouette."

# game/script.rpy:12256
translate spanish_mx start_f34f5712:

    # narrator2 "I place a hand on her shoulder."
    narrator2 "Coloco una mano sobre su hombro."

# game/script.rpy:12257
translate spanish_mx start_883d9f5d:

    # al2 "Auntie…"
    al2 "Tía…"

# game/script.rpy:12258
translate spanish_mx start_fe583d32:

    # am "The adults are going to be busy from now on."
    am "Los adultos estarán ocupados a partir de ahora."

# game/script.rpy:12259
translate spanish_mx start_3cfade7e:

    # am "We need you to watch the house and take care of Linette and Dia while we’re gone."
    am "Necesitamos que vigiles la casa y cuides de Linette y Dia mientras no estamos."

# game/script.rpy:12260
translate spanish_mx start_77af1deb:

    # am "It’s an important job.{w=0.25} Can you do it?"
    am "Es una tarea importante.{w=0.25} ¿Puedes hacerlo?"

# game/script.rpy:12261
translate spanish_mx start_d402230c:

    # narrator2 "She nods back at me, her expression regaining a bit of color."
    narrator2 "Me asiente en respuesta, su expresión recuperando un poco de color."

# game/script.rpy:12262
translate spanish_mx start_009a1422:

    # al2 "Alouette— will do her best."
    al2 "Alouette— hará su mejor esfuerzo."

# game/script.rpy:12263
translate spanish_mx start_f3cc780b:

    # narrator2 "Giving people a responsibility in a crisis forces them to compartmentalize."
    narrator2 "Dar a la gente una responsabilidad en una crisis los obliga a compartimentar."

# game/script.rpy:12264
translate spanish_mx start_8013214f:

    # narrator2 "The feeling that one had some control over the situation— it was an essential illusion."
    narrator2 "La sensación de tener algo de control sobre la situación— era una ilusión esencial."

# game/script.rpy:12265
translate spanish_mx start_6a06e7b6:

    # al2 "Please… please make Linny and Dia— better."
    al2 "Por favor… haz que Linny y Dia— se mejoren."

# game/script.rpy:12266
translate spanish_mx start_c916f80a:

    # am "I will."
    am "Lo haré."

# game/script.rpy:12267
translate spanish_mx start_d0b2c97c:

    # narrator2 "Eyes full of innocent hope."
    narrator2 "Ojos llenos de inocente esperanza."

# game/script.rpy:12268
translate spanish_mx start_5ae71a64:

    # narrator2 "With that image burned into my mind, I set off towards my next destination."
    narrator2 "Con esa imagen impresa en mi mente, me marcho hacia mi siguiente destino."

# game/script.rpy:12275
translate spanish_mx start_f06fdd46:

    # narrator2 "Aachen Neues— a state of the art hospital that had been renovated with the latest of UL technology."
    narrator2 "Aachen Neues— un hospital de vanguardia que había sido renovado con la más nueva tecnología TD."

# game/script.rpy:12276
translate spanish_mx start_78c7a0da:

    # narrator2 "For many in Socotrine, it stood as a symbol of hope."
    narrator2 "Para muchos en Socotrine, representaba un símbolo de esperanza."

# game/script.rpy:12277
translate spanish_mx start_4f88f8cc:

    # narrator2 "It had served as the forefront of the Verloren Master’s successful campaign to eradicate the plague after all."
    narrator2 "Después de todo, había servido como el frente de la exitosa campaña del Capitán Verloren para erradicar la peste."

# game/script.rpy:12278
translate spanish_mx start_07cfd07d:

    # narrator2 "As expected, scores of people had already gathered in front of the main entrance."
    narrator2 "Como esperaba, decenas de personas ya se habían aglomerado frente a la entrada principal."

# game/script.rpy:12279
translate spanish_mx start_1f5a290d:

    # narrator2 "Slowing my pace, I drift through the crowd, catching bits of conversation as I pass by."
    narrator2 "Ralentizando mi ritmo, camino a través de la multitud, escuchando pedazos de conversación mientras lo hago."

# game/script.rpy:12280
translate spanish_mx start_a04abc49:

    # to "—heard? Seems they already know a way to treat the burns."
    to "—chaste? Parece que ya saben cómo tratar las quemaduras."

# game/script.rpy:12281
translate spanish_mx start_37249ce2:

    # to "Something similar happened back in their homeland—"
    to "Algo similar pasó en su tierra natal—"

# game/script.rpy:12282
translate spanish_mx start_933313e2:

    # to "—calling it ‘The Fire of Collapse…’"
    to "—llamaron ‘El Fuego del Colapso…’"

# game/script.rpy:12283
translate spanish_mx start_73387bb6:

    # to "I know. But why did it just blow out the other day—"
    to "Lo sé. Pero, ¿por qué ocurrió de la nada el otro día—"

# game/script.rpy:12284
translate spanish_mx start_a2982fb1:

    # to "—of about twenty people entered through the—"
    to "—de alrededor veinte personas entraron por—"

# game/script.rpy:12285
translate spanish_mx start_1605ed8f:

    # to "…come back yet?"
    to "…no han regresado?"

# game/script.rpy:12286
translate spanish_mx start_87a5a1e0:

    # narrator2 "Apparently a cure already existed."
    narrator2 "Aparentemente, una cura ya existía."

# game/script.rpy:12287
translate spanish_mx start_41bb232e:

    # narrator2 "That seemed too convenient to be true though."
    narrator2 "Pero eso sonaba demasiado conveniente para ser verdad."

# game/script.rpy:12288
translate spanish_mx start_b28f649f:

    # narrator2 "Heeding my intuition, I make several rounds of the perimeter and commit the layout to memory."
    narrator2 "Siguiendo mi intuición, doy varias vueltas al perímetro y memorizo la disposición del lugar."

# game/script.rpy:12289
translate spanish_mx start_12c7fa06:

    # narrator2 "It seemed victims with conventional injuries were being accepted at the side entrances, while only those with “deviant burns” were being admitted through the main one."
    narrator2 "Parecía que las víctimas con heridas convencionales estaban siendo aceptadas por las entradas a los costados, mientras que aquellos con “quemaduras anormales” eran admitidos por la principal."

# game/script.rpy:12290
translate spanish_mx start_57ccad07:

    # am "Why is security so tight...?"
    am "¿Por qué está tan estricta la seguridad...?"

# game/script.rpy:12291
translate spanish_mx start_74ed4157:

    # narrator2 "Squads of the Socotroni Guard were stationed everywhere.\nEven the surrounding streets were heavily fortified."
    narrator2 "Escuadrones de la Guardia Socotrina estaban estacionados por todos lados. Incluso las calles aledañas estaban fuertemente fortificadas."

# game/script.rpy:12292
translate spanish_mx start_d4952c96:

    # narrator2 "Considering the dire straits of the rest of Socotrine, it was odd they had mobilized such manpower here."
    narrator2 "Considerando las graves dificultades del resto de Socotrine, era extraño que hubiesen movilizado a tanto personal aquí."

# game/script.rpy:12293
translate spanish_mx start_83acebb0:

    # narrator2 "As I finish a rotation around the complex, I overhear a commotion at the main entrance."
    narrator2 "Al terminar una vuelta alrededor del complejo, escucho una conmoción en la entrada principal."

# game/script.rpy:12294
translate spanish_mx start_1e7c07d6:

    # wo "Why can’t I go in?{w=0.2} My little boy is in there."
    wo "¿Por qué no puedo entrar?{w=0.2} Mi pequeño hijo está ahí."

# game/script.rpy:12295
translate spanish_mx start_258e5ac9:

    # gu "We’ve already made this clear to you.\nEntry is prohibited to all but patients and medical personnel."
    gu "Ya se lo habíamos dejado claro.\nLa entrada está prohibida para todos excepto pacientes y personal médico."

# game/script.rpy:12296
translate spanish_mx start_34a0aae8:

    # wo "Please.{w=0.15} I just want to know if he’s doing alright…!"
    wo "Por favor.{w=0.15} ¡Solo quiero saber si está bien…!"

# game/script.rpy:12297
translate spanish_mx start_8a572649:

    # to "Wait.{w=0.25} Is that them?"
    to "Espera.{w=0.25} ¿Son ellos?"

# game/script.rpy:12298
translate spanish_mx start_0a9e2d07:

    # to "L-Look! They’re coming out!"
    to "¡M-Mira! ¡Están saliendo!"

# game/script.rpy:12299
translate spanish_mx start_cd0f9afa:

    # narrator2 "The crowd stirs as a column of people emerges from the hospital."
    narrator2 "La multitud se agita mientras una fila de personas emerge del hospital."

# game/script.rpy:12300
translate spanish_mx start_d35bba5b:

    # narrator2 "All of them were scarred with the etchings unique to the rainbow fire."
    narrator2 "Todos estaban heridos con las cicatrices únicas del fuego arcoíris."

# game/script.rpy:12301
translate spanish_mx start_b4fb0cfe_1:

    # narrator2 "However…"
    narrator2 "Sin embargo…"

# game/script.rpy:12302
translate spanish_mx start_09f20715:

    # to "Their burns look different, don’t they?"
    to "Sus quemaduras se ven diferentes, ¿no?"

# game/script.rpy:12303
translate spanish_mx start_134af839:

    # to "They’re really cured…!"
    to "¡De verdad están curados…!"

# game/script.rpy:12304
translate spanish_mx start_74ab6b39:

    # narrator2 "Something wasn’t right though."
    narrator2 "Pero algo no estaba bien."

# game/script.rpy:12305
translate spanish_mx start_48a5f006:

    # narrator2 "Of the twenty that had supposedly gone in, only around half had returned."
    narrator2 "De las veinte personas que supuestamente habían entrado, solo regresó alrededor de la mitad."

# game/script.rpy:12306
translate spanish_mx start_a38d29b7:

    # wo "What do you mean?!{w=0.15} Can’t you just cure him like the rest?"
    wo "¡¿De qué habla?!{w=0.15} ¿No puede solo curarlo como al resto?"

# game/script.rpy:12307
translate spanish_mx start_ed3da72a:

    # narrator2 "The mother from before was shouting franticly at a doctor accompanying the treated patients."
    narrator2 "La madre de antes gritaba frenéticamente al doctor que acompañaba a los pacientes tratados."

# game/script.rpy:12308
translate spanish_mx start_c492bf30:

    # doc "Please understand.{p=0.25}Your son’s injuries are very intensive in comparison."
    doc "Por favor, entienda.{p=0.25}Las heridas de su hijo son demasiado intensivas en comparación."

# game/script.rpy:12309
translate spanish_mx start_f3531770:

    # doc "He is in critical condition and as such we need to continue monitoring—"
    doc "Está en condición crítica y como tal necesitamos continuar monitoreando—"

# game/script.rpy:12310
translate spanish_mx start_c4550e21:

    # wo "Why won’t you listen to me?{p=0.15}He’s such a skittish boy…"
    wo "¿Por qué no me escucha?{p=0.15} Es un niño muy temeroso…"

# game/script.rpy:12311
translate spanish_mx start_2b1581b2:

    # wo "Waking up in a place he doesn’t know, surrounded by strangers…{w=0.3} he’ll be so scared."
    wo "Despertarse en un lugar que no conoce, rodeado de extraños…{w=0.3} se asustará mucho."

# game/script.rpy:12312
translate spanish_mx start_f9c28ab3:

    # wo "That’s why I need to be there for him…!"
    wo "¡Por eso necesito estar ahí con él…!"

# game/script.rpy:12313
translate spanish_mx start_4c2aa6d5:

    # doc "..."
    doc "..."

# game/script.rpy:12314
translate spanish_mx start_54d274af:

    # doc "I’m sorry ma’am…"
    doc "Lo siento, señora."

# game/script.rpy:12315
translate spanish_mx start_53bd28cf:

    # wo "Why are you apologizing?"
    wo "¿Por qué se disculpa?"

# game/script.rpy:12316
translate spanish_mx start_b52b41df:

    # wo "Just let me in!" with Shake((0, 0, 0, 0), 0.5, dist=7)
    wo "¡Solo déjeme entrar!" with Shake((0, 0, 0, 0), 0.5, dist=7)

# game/script.rpy:12317
translate spanish_mx start_e669ca61:

    # wo "Let me see him!" with Shake((0, 0, 0, 0), 0.5, dist=10)
    wo "¡Déjeme ver a mi hijo!" with Shake((0, 0, 0, 0), 0.5, dist=10)

# game/script.rpy:12318
translate spanish_mx start_c72a82f5:

    # narrator2 "The mother pushes past the doctor and tries to enter the hospital, only to be held back by the officer from before."
    narrator2 "La madre empuja al doctor e intenta entrar al hospital, solo para ser detenida por el oficial de antes."

# game/script.rpy:12319
translate spanish_mx start_8366be01:

    # gu "Stop right there!"
    gu "¡Alto ahí!"

# game/script.rpy:12320
translate spanish_mx start_43f19f91:

    # wo "L-Let me go!!{p=0.2}My son…{w=0.2} my son is in there!"
    wo "¡¡D-déjeme ir!!{p=0.2}Mi hijo…{w=0.2} ¡mi hijo está ahí!"

# game/script.rpy:12324
translate spanish_mx start_20d7a779:

    # gu "{i}Gah!{/i}" with Shake((0, 0, 0, 0), 0.7, dist=9)
    gu "{i}¡Gah!{/i}" with Shake((0, 0, 0, 0), 0.7, dist=9)

# game/script.rpy:12325
translate spanish_mx start_263f2e85:

    # narrator2 "The officer clutches at his wrist as the woman flails wildly under his grasp."
    narrator2 "El oficial se sujeta la muñeca mientras la mujer se agita bajo su agarre."

# game/script.rpy:12326
translate spanish_mx start_bee61955:

    # gu "She bit me!"
    gu "¡Me mordió!"

# game/script.rpy:12330
translate spanish_mx start_966df926:

    # narrator2 "Unsheathing his sword, he slams his hilt against the woman’s head and drives her to the ground." with Shake((0, 0, 0, 0), 0.3, dist=10)
    narrator2 "Desenvainando su espada, golpea la empuñadura contra la cabeza de la mujer y la lleva al suelo." with Shake((0, 0, 0, 0), 0.3, dist=10)

# game/script.rpy:12331
translate spanish_mx start_03c4db62:

    # narrator2 "But even then, she doesn’t stop trying to enter."
    narrator2 "Pero aun así, no deja de intentar hacerse paso."

# game/script.rpy:12332
translate spanish_mx start_0fdada66:

    # gu "How many times do I have to tell you before you get it through your head?"
    gu "¿Cuántas veces tengo que decírtelo hasta que te entre en la cabeza?"

# game/script.rpy:12333
translate spanish_mx start_a3df72a0:

    # gu "You.{w=0.2} Can’t.{w=0.2} Go.{w=0.2} In!"
    gu "No.{w=0.2} Puedes.{w=0.2} ¡Pasar!"

# game/script.rpy:12337
translate spanish_mx start_5a678a67:

    # narrator2 "Enraged, the officer begins to kick and stomp the crawling woman mercilessly." with Shake((0, 0, 0, 0), 1.0, dist=10)
    narrator2 "Enfurecido, el oficial empieza a patear y pisar a la desamparada mujer sin piedad." with Shake((0, 0, 0, 0), 1.0, dist=10)

# game/script.rpy:12338
translate spanish_mx start_e3a51ff0:

    # wo "Let me see him…!{w=0.3}{nw} " with Shake((0, 0, 0, 0), 0.25, dist=7)
    wo "¡Déjeme verlo…!{w=0.3}{nw} " with Shake((0, 0, 0, 0), 0.25, dist=7)

# game/script.rpy:12339
translate spanish_mx start_aa24eb9f:

    # extend "Let me see him!!" with Shake((0, 0, 0, 0), 0.45, dist=8)
    extend "¡¡Déjeme verlo!!" with Shake((0, 0, 0, 0), 0.45, dist=8)

# game/script.rpy:12340
translate spanish_mx start_cea23b78:

    # narrator2 "The woman breaks into sobs as she tries to shield herself from the crushing blows."
    narrator2 "La mujer empieza a llorar mientras intenta protegerse de los duros golpes."

# game/script.rpy:12341
translate spanish_mx start_b4563c98:

    # wo "Nell…!{w=0.3}{nw} " with Shake((0, 0, 0, 0), 0.25, dist=7)
    wo "¡Nell…!{w=0.3}{nw} " with Shake((0, 0, 0, 0), 0.25, dist=7)

# game/script.rpy:12342
translate spanish_mx start_d29b2c27:

    # extend "Nell!!{w=0.15} Ne-" with Shake((0, 0, 0, 0), 0.35, dist=5)
    extend "¡¡Nell!!{w=0.15} Ne-" with Shake((0, 0, 0, 0), 0.35, dist=5)

# game/script.rpy:12346
translate spanish_mx start_e7c44ad5:

    # gu "Shut up! Shut up! SHUT UP!" with Shake((0, 0, 0, 0), 1.7, dist=10)
    gu "¡Cállate! ¡Cállate! ¡CÁLLATE!" with Shake((0, 0, 0, 0), 1.7, dist=10)

# game/script.rpy:12350
translate spanish_mx start_498ce908:

    # narrator2 "The officer brings his foot down hard, silencing her as she tries to call out a name." with Shake((0, 0, 0, 0), 0.3, dist=13)
    narrator2 "El oficial deja caer su pie con fuerza, silenciándola mientras intenta llamar un nombre." with Shake((0, 0, 0, 0), 0.3, dist=13)

# game/script.rpy:12351
translate spanish_mx start_fee2f26a:

    # gu2 "Stop it!" with Shake((0, 0, 0, 0), 0.5, dist=15)
    gu2 "¡Basta!" with Shake((0, 0, 0, 0), 0.5, dist=15)

# game/script.rpy:12355
translate spanish_mx start_b84c6adb:

    # narrator2 "With an angry growl the officer’s partner grabs him by the collar and shoves him away." with Shake((0, 0, 0, 0), 0.3, dist=6)
    narrator2 "Con un gruñido enfadado el compañero del oficial lo toma del cuello y lo empuja hacia atrás." with Shake((0, 0, 0, 0), 0.3, dist=6)

# game/script.rpy:12356
translate spanish_mx start_062b11eb:

    # narrator2 "But the woman had already been rendered motionless in the dirt."
    narrator2 "Pero la mujer ya se encontraba inmóvil en la tierra."

# game/script.rpy:12357
translate spanish_mx start_88bf494d:

    # gu2 "What the hell do you think you’re doing?!"
    gu2 "¡¿Qué demonios crees que estás haciendo?!"

# game/script.rpy:12358
translate spanish_mx start_530a38bd:

    # gu "Tch.{w=0.25} I can’t stand people like her."
    gu "Tch.{w=0.25} No soporto a gente como ella."

# game/script.rpy:12359
translate spanish_mx start_221a22b8:

    # gu "They act like they’re the only one hurting in the world…"
    gu "Hacen como si fueran los únicos con problemas en el mundo…"

# game/script.rpy:12360
translate spanish_mx start_6f238986:

    # gu "It makes me sick."
    gu "Me enferma."

# game/script.rpy:12361
translate spanish_mx start_73f967ab:

    # narrator2 "Turning his back on the woman, the officer spits into the dirt."
    narrator2 "Dando la espalda a la mujer, el oficial escupe en la tierra."

# game/script.rpy:12362
translate spanish_mx start_5b9144dc:

    # gu2 "…You can’t take it out on the people."
    gu2 "…No puedes desquitarte con la gente."

# game/script.rpy:12363
translate spanish_mx start_7d1af720:

    # gu2 "Many of them are going through the same thing you know."
    gu2 "Muchos de ellos están pasando por lo mismo, ¿sabes?"

# game/script.rpy:12364
translate spanish_mx start_521f7331:

    # gu "..."
    gu "..."

# game/script.rpy:12365
translate spanish_mx start_5063c60a:

    # gu2 "Take a break and cool off.{w=0.25} I’ll get someone to cover."
    gu2 "Toma un descanso y enfríate la cabeza.{w=0.25} Llamaré a alguien para cubrirte."

# game/script.rpy:12366
translate spanish_mx start_87ebd175:

    # gu2 "Doctor, can we get her a stretcher and take her through the side entrance?"
    gu2 "Doctor, ¿podríamos conseguir una camilla y llevarla por la entrada lateral?"

# game/script.rpy:12367
translate spanish_mx start_dfa2f458:

    # doc "Y-Yes, I’ll get someone on it right away."
    doc "S-sí, pondré a alguien en ello de inmediato."

# game/script.rpy:12368
translate spanish_mx start_0e4b9fd1:

    # narrator2 "Meanwhile, a palpable tension had built up in the on-looking crowd."
    narrator2 "Mientras tanto, una tensión palpable se había acumulado en la multitud que había presenciado el acto."

# game/script.rpy:12369
translate spanish_mx start_f4d27e8f:

    # to "How could he do that to her?"
    to "¿Cómo pudo hacerle eso?"

# game/script.rpy:12370
translate spanish_mx start_2c72fa84:

    # to "Are they hiding something?"
    to "¿Están ocultando algo?"

# game/script.rpy:12371
translate spanish_mx start_2c8be40d:

    # to "Why can’t we go in?"
    to "¿Por qué no podemos entrar?"

# game/script.rpy:12372
translate spanish_mx start_fbd4742c:

    # narrator2 "The guards on post tighten their formation as they bear the brunt of the crowd’s resentment."
    narrator2 "Los guardias en puesto refuerzan su formación mientras reciben el peso de los resentimientos de la multitud."

# game/script.rpy:12373
translate spanish_mx start_41a3a766:

    # doc "Attention all victims suffering deviant burns from the Fire of Collapse:{p=0.25}please line up in an orderly fashion."
    doc "Atención a todas las víctimas que sufren de quemaduras anormales por el Fuego del Colapso.{w=0.25} Formen una fila de manera ordenada."

# game/script.rpy:12374
translate spanish_mx start_df079a81:

    # doc "Stretchers will be provided for the incapacitated."
    doc "Se proporcionarán camillas para los incapacitados."

# game/script.rpy:12378
translate spanish_mx start_f5399257:

    # narrator2 "For the remainder of the day, I continue to observe the hospital’s activities."
    narrator2 "Por el resto del día, continúo observando las actividades del hospital."

# game/script.rpy:12379
translate spanish_mx start_339efc65:

    # narrator2 "However, the same trend only continues to persist."
    narrator2 "Sin embargo, el mismo patrón continúa persistiendo."

# game/script.rpy:12380
translate spanish_mx start_d04f6b2d:

    # narrator2 "Thirty people would enter but only fourteen would exit."
    narrator2 "Treinta personas entraban pero solo catorce salían."

# game/script.rpy:12381
translate spanish_mx start_d8c35aa3:

    # narrator2 "Forty would go in but only twenty would return."
    narrator2 "Cuarenta entraban pero solo veinte regresaban."

# game/script.rpy:12382
translate spanish_mx start_bed72630:

    # narrator2 "Like clockwork, the cycle repeats.{w=0.25} Again and again."
    narrator2 "Como maquinaria, el ciclo se repite.{w=0.25} Una y otra vez."

# game/script.rpy:12383
translate spanish_mx start_8fc10b67:

    # narrator2 "Again and again."
    narrator2 "Una y otra vez."

# game/script.rpy:12384
translate spanish_mx start_006478f1:

    # am "So it’s come to this…"
    am "Así que ha llegado a esto…"

# game/script.rpy:12385
translate spanish_mx start_f161a70a:

    # narrator2 "Entrusting Callia and Autune’s lives to odds I didn’t understand was unthinkable."
    narrator2 "Encargar las vidas de Callia y Autune a probabilidades que no comprendía era impensable."

# game/script.rpy:12386
translate spanish_mx start_3e3da915:

    # narrator2 "There wasn’t anything else I could discern from outside though."
    narrator2 "Pero no había nada más que pudiera discernir desde fuera."

# game/script.rpy:12387
translate spanish_mx start_cc8a82e9:

    # narrator2 "If I wanted to learn more…"
    narrator2 "Si quería aprender más…"

# game/script.rpy:12388
translate spanish_mx start_a1fe86ec:

    # narrator2 "I would have to infiltrate the building."
    narrator2 "Tendría que infiltrarme en el edificio."

# game/script.rpy:12397
translate spanish_mx start_b6bbcd75:

    # narrator2 "Callia and Autune were getting worse by the day."
    narrator2 "Callia y Autune empeoraban día con día."

# game/script.rpy:12398
translate spanish_mx start_a1865b70:

    # narrator2 "Autune’s burn spread had already extended up his forearms."
    narrator2 "Las quemaduras de Autune se habían extendido hasta sus antebrazos."

# game/script.rpy:12399
translate spanish_mx start_42173cf6:

    # narrator2 "And Callia wasn’t faring much better."
    narrator2 "Y a Callia no le iba mejor."

# game/script.rpy:12400
translate spanish_mx start_fd2b7054:

    # narrator2 "That reality weighs down on me as I make preparations to infiltrate the hospital."
    narrator2 "La realidad pesa sobre mí mientras hago las preparaciones para infiltrarme en el hospital."

# game/script.rpy:12401
translate spanish_mx start_cf919b0e:

    # narrator2 "After smearing a layer of soot over my left arm, I wrap it up in bandages."
    narrator2 "Tras untar una capa de hollín sobre mi brazo izquierdo, lo envuelvo con vendas."

# game/script.rpy:12402
translate spanish_mx start_73944852:

    # narrator2 "Having observed their system several times already, I had determined that only a basic preliminary check was given at the entrance."
    narrator2 "Al haber observado su sistema ya múltiples veces, había determinado que solo se hacía una inspección básica preliminar en la entrada."

# game/script.rpy:12403
translate spanish_mx start_dc2d3130:

    # narrator2 "Though they’ll probably look more intensively later, I’ll already be inside by then."
    narrator2 "Aunque quizás examinarían más intensivamente después, ya estaría dentro para entonces."

# game/script.rpy:12404
translate spanish_mx start_243adaa0:

    # narrator2 "Upon completing my masquerade, I line up in the next wave of patients."
    narrator2 "Tras completar mi farsa, hago fila en la siguiente oleada de pacientes."

# game/script.rpy:12405
translate spanish_mx start_47456110:

    # narrator2 "When it comes to my turn to be inspected though, the doctor seems a bit wary."
    narrator2 "Pero cuando llega mi turno de ser inspeccionada, el doctor se ve cauteloso."

# game/script.rpy:12406
translate spanish_mx start_2ff08147:

    # doc "Did that hurt?"
    doc "¿Eso dolió?"

# game/script.rpy:12407
translate spanish_mx start_2754546b:

    # narrator2 "He asks after poking my faux wound."
    narrator2 "Pregunta tras tocar mi herida artificial."

# game/script.rpy:12408
translate spanish_mx start_eb3628a7:

    # am "I didn’t feel anything."
    am "No sentí nada."

# game/script.rpy:12409
translate spanish_mx start_4c0624e7:

    # narrator2 "Thinking quickly, I answer in a way that seems unintuitive."
    narrator2 "Pensando rápido, respondo de una forma que parece poco intuitiva."

# game/script.rpy:12410
translate spanish_mx start_e00ecf7f:

    # am "Are you sure you touched me?"
    am "¿Está seguro de que me tocó?"

# game/script.rpy:12411
translate spanish_mx start_99f9a1c8:

    # doc "Go on ahead."
    doc "Pase."

# game/script.rpy:12412
translate spanish_mx start_4e3e0e6f:

    # narrator2 "The doctor nods before turning his attention to the next patient."
    narrator2 "El doctor asiente antes de dirigir su atención al siguiente paciente."

# game/script.rpy:12413
translate spanish_mx start_24083e77:

    # narrator2 "Areas marked by the rainbow fire seemed to lose all feeling after all."
    narrator2 "Después de todo, las áreas marcadas por el fuego arcoíris parecían perder toda sensación."

# game/script.rpy:12414
translate spanish_mx start_fda2928c:

    # narrator2 "It was only when the deviant burn spread that the victim experienced pain."
    narrator2 "Era solo cuando la quemadura anormal se esparcía que la víctima experimentaba dolor."

# game/script.rpy:12415
translate spanish_mx start_38550d82:

    # narrator2 "I had learned that much from observing Callia and Autune."
    narrator2 "Había aprendido eso de observar a Callia y a Autune."

# game/script.rpy:12419
translate spanish_mx start_28220315:

    # narrator2 "After passing the entrance checkpoint, our group gets cleared to enter."
    narrator2 "Tras pasar el punto de control de la entrada, a un grupo se le permite entrar."

# game/script.rpy:12423
translate spanish_mx start_35a52d87:

    # narrator2 "Several details stand out to me as we’re led deep into the building."
    narrator2 "Algunos detalles me llaman la atención mientras se nos guía hacia lo profundo del edificio."

# game/script.rpy:12424
translate spanish_mx start_3d814b61:

    # narrator2 "Though it sounded like there was significant activity in the distance, the area we were being led through was utterly empty."
    narrator2 "Aunque sonaba a que había significativa actividad a la distancia, el área por la que nos guiaban estaba completamente vacía."

# game/script.rpy:12425
translate spanish_mx start_292f9e57:

    # narrator2 "Even security detail was absent."
    narrator2 "Incluso el personal de seguridad estaba ausente."

# game/script.rpy:12426
translate spanish_mx start_7210c17d:

    # narrator2 "It seemed the guards weren’t authorized to enter this area."
    narrator2 "Parecía que los guardias no estaban autorizados para entrar a esta área."

# game/script.rpy:12427
translate spanish_mx start_8c610e37:

    # narrator2 "Moving around will still be difficult at this rate though."
    narrator2 "Moverse libremente será difícil a este paso."

# game/script.rpy:12428
translate spanish_mx start_ed23a90f:

    # narrator2 "After being brought to a large junction, individual nurses begin taking us one by one into separate rooms."
    narrator2 "Tras ser traídos a una gran intersección, muchas enfermeras individuales comienzan a llevarnos uno por uno a cuartos distintos."

# game/script.rpy:12429
translate spanish_mx start_c7659569:

    # narrator2 "The nurse assigned to me was wearing a surgical mask and had an average build."
    narrator2 "La enfermera que se me asignó llevaba un cubrebocas y era de complexión media."

# game/script.rpy:12434
translate spanish_mx start_24a602c6:

    # narrator2 "After leading me into a check-up room she closes the door behind us."
    narrator2 "Tras guiarme a un cuarto de examinación, cierra la puerta tras nosotras."

# game/script.rpy:12435
translate spanish_mx start_dba66c34:

    # narrator2 "A raised stretcher, a counter with a faucet, and a closet—{w=0.25} perfect."
    narrator2 "Una camilla elevada, una mesa con un grifo, y un closet—{w=0.25} perfecto."

# game/script.rpy:12436
translate spanish_mx start_f4694430:

    # nu "Okay ma’am."
    nu "Bien, señorita."

# game/script.rpy:12437
translate spanish_mx start_6f5f549f:

    # nu "Could you please remove the bandage on your arm and show me your burn?"
    nu "¿Podría por favor quitar la venda en su brazo y mostrarme su quemadura?"

# game/script.rpy:12438
translate spanish_mx start_9b66d633:

    # narrator2 "She begins preparing a syringe on the nearby counter."
    narrator2 "Comienza a preparar una jeringa en la mesa cercana."

# game/script.rpy:12439
translate spanish_mx start_b8cc394e:

    # am "What’s in the syringe?"
    am "¿Qué hay en la jeringa?"

# game/script.rpy:12440
translate spanish_mx start_7bb850d5:

    # nu "This is a mild relaxant meant to calm you down before the procedure.\nIt’ll just be a little-"
    nu "Es un relajante ligero que la tranquilizará antes del procedimiento.\nSolo será un-"

# game/script.rpy:12441
translate spanish_mx start_b2a760a8:

    # narrator2 "After finishing her preparations she tries to turn around."
    narrator2 "Al terminar sus preparaciones intenta darse la vuelta."

# game/script.rpy:12442
translate spanish_mx start_1d07f1f5:

    # narrator2 "But I don’t let her."
    narrator2 "Pero no se lo permito."

# game/script.rpy:12446
translate spanish_mx start_3d1c5b57:

    # narrator2 "I grab her by the wrist and press down on her shoulder blade, pinning her against the counter." with Shake((0, 0, 0, 0), 0.3, dist=6)
    narrator2 "La tomo de la muñeca y presiono en su omóplato, fijándola contra la mesa." with Shake((0, 0, 0, 0), 0.3, dist=6)

# game/script.rpy:12447
translate spanish_mx start_98f920e3:

    # nu "What… are you-"
    nu "Qué… estás-"

# game/script.rpy:12451
translate spanish_mx start_1a152a9a:

    # narrator2 "Twisting the syringe out of her hand, I wrap my arm around her neck and cut off her circulation." with Shake((0, 0, 0, 0), 0.5, dist=5)
    narrator2 "Haciéndole tirar la jeringa retorciendo su mano, envuelvo mi brazo alrededor de su cuello y detengo su circulación." with Shake((0, 0, 0, 0), 0.5, dist=5)

# game/script.rpy:12452
translate spanish_mx start_d4426dbe:

    # nu "Mmphf…!" with Shake((0, 0, 0, 0), 0.3, dist=3)
    nu "¡Mmff…!" with Shake((0, 0, 0, 0), 0.3, dist=3)

# game/script.rpy:12456
translate spanish_mx start_4bcee86c:

    # narrator2 "Maintaining the choke until she goes limp, I insert the tip of the syringe into one of her bulging blood vessels and press down on the plunger." with Shake((0, 0, 0, 0), 0.4, dist=4)
    narrator2 "Manteniendo la posición hasta que se queda sin fuerzas, inserto la punta de la jeringa a una de sus venas protuberantes y presiono el émbolo." with Shake((0, 0, 0, 0), 0.4, dist=4)

# game/script.rpy:12457
translate spanish_mx start_861d4e78:

    # narrator2 "“Mild relaxant” was an understatement."
    narrator2 "“Relajante ligero” era un eufemismo."

# game/script.rpy:12458
translate spanish_mx start_66a77f43:

    # narrator2 "I had known from the beginning the sedative was intended to knock out the patient."
    narrator2 "Sabía desde el principio que el sedante tenía el propósito de derribar al paciente."

# game/script.rpy:12459
translate spanish_mx start_5a508073:

    # narrator2 "After all, none of the mended victims outside had been able to describe the treatment process."
    narrator2 "Después de todo, ninguna de las víctimas curadas de afuera fue capaz de describir el proceso del tratamiento."

# game/script.rpy:12460
translate spanish_mx start_26b84c64:

    # narrator2 "They had all said they’d fallen asleep and woken up cured."
    narrator2 "Todos decían que se quedaron dormidos y despertaron curados."

# game/script.rpy:12464
translate spanish_mx start_45067c00:

    # narrator2 "After stripping the nurse of her uniform, I tie her up using some cravats I find in the closet and gag her using the bandage on my arm."
    narrator2 "Tras quitarle el uniforme a la enfermera, la ato usando algunos pañuelos que encuentro en el closet y tapo su boca usando la venda de mi brazo."

# game/script.rpy:12465
translate spanish_mx start_5f5d9dcc:

    # narrator2 "Then, I place her in the closet in the recovery position, wash the soot from my arm, and don her uniform."
    narrator2 "Después, la coloco en el closet en la posición lateral de seguridad, lavo el hollín de mi brazo, y visto su uniforme."

# game/script.rpy:12466
translate spanish_mx start_f70a5bcd:

    # narrator2 "Complete with mask and cap, it served as an excellent disguise."
    narrator2 "Completo con cubrebocas y gorro, servía como un excelente disfraz."

# game/script.rpy:12467
translate spanish_mx start_f8eb1b6f:

    # narrator2 "So far everything had gone as planned."
    narrator2 "Hasta ahora todo había salido acorde al plan."

# game/script.rpy:12468
translate spanish_mx start_48971bdb:

    # narrator2 "But from here on out I’ll have to improvise."
    narrator2 "Pero de aquí en adelante tendré que improvisar."

# game/script.rpy:12469
translate spanish_mx start_4a759a3e:

    # narrator2 "Bringing my ear to the door, I try to gauge the situation outside."
    narrator2 "Llevando mi oído a la puerta, intento evaluar la situación afuera."

# game/script.rpy:12470
translate spanish_mx start_8af2bec8:

    # narrator2 "It isn’t until several minutes pass that a general bustle begins to pick up."
    narrator2 "No es hasta que pasan muchos minutos que un ajetreo comienza a formarse."

# game/script.rpy:12474
translate spanish_mx start_c17299fb:

    # narrator2 "Choosing that moment to act, I enter the hallway."
    narrator2 "Eligiendo el momento para actuar, entro al pasillo."

# game/script.rpy:12475
translate spanish_mx start_795b39a6:

    # narrator2 "Upon exiting, I find myself amidst a stream of nurses carrying stretchers of the deviant burn victims."
    narrator2 "Al salir, me encuentro entre un flujo de enfermeras cargando camillas con las víctimas de quemaduras anormales."

# game/script.rpy:12476
translate spanish_mx start_b4c4fc52:

    # narrator2 "In addition to being rendered unconscious the patients had been stripped naked, exposing their injuries to plain view."
    narrator2 "Además de haberlos dejado inconscientes, se les había desvestido, exponiendo sus heridas a la vista."

# game/script.rpy:12477
translate spanish_mx start_2f4d2782:

    # narrator2 "I begin moving down the hall."
    narrator2 "Comienzo a caminar a lo largo del pasillo."

# game/script.rpy:12478
translate spanish_mx start_7d90a1ad:

    # narrator2 "Glancing into the rooms as I pass, I spot a lone nurse at the head of a stretcher."
    narrator2 "Viendo de reojo los cuartos por los que paso, encuentro a una enfermera sola en la cabecera de una camilla."

# game/script.rpy:12482
translate spanish_mx start_27727e4c:

    # narrator2 "As if it were completely natural, I step in and grab the other end."
    narrator2 "Como si fuera completamente natural, me acerco y sujeto el otro extremo."

# game/script.rpy:12483
translate spanish_mx start_f53f24c9:

    # nu "3…{w=0.2} 2…{w=0.2} 1…"
    nu "3…{w=0.2} 2…{w=0.2} 1…"

# game/script.rpy:12487
translate spanish_mx start_974854b7:

    # narrator2 "The nurse counts off before lifting in sync with me." with Shake((0, 0, 0, 0), 0.2, dist=3)
    narrator2 "La enfermera cuenta atrás antes de levantar en sincronía conmigo." with Shake((0, 0, 0, 0), 0.2, dist=3)

# game/script.rpy:12491
translate spanish_mx start_c7c44cae:

    # narrator2 "Letting her take point, we join the flow of traffic outside."
    narrator2 "Dejándola guiar el camino, nos unimos al flujo de tráfico afuera."

# game/script.rpy:12492
translate spanish_mx start_760a6259:

    # nu "This one’s a really young one isn’t she?"
    nu "Esta es muy joven, ¿no lo crees?"

# game/script.rpy:12493
translate spanish_mx start_58eb857a:

    # narrator2 "Though it goes well initially, the nurse suddenly starts speaking to me."
    narrator2 "Aunque va bien al principio, la enfermera empieza a hablarme de repente."

# game/script.rpy:12494
translate spanish_mx start_8afbe0f1_1:

    # am "Yeah."
    am "Sí."

# game/script.rpy:12495
translate spanish_mx start_52221450:

    # narrator2 "I respond ambiguously as I look down at the patient on the stretcher."
    narrator2 "Respondo ambiguamente mientras observo al paciente en la camilla."

# game/script.rpy:12496
translate spanish_mx start_b96f1fca:

    # narrator2 "It was as the nurse said."
    narrator2 "Era como la enfermera dijo."

# game/script.rpy:12497
translate spanish_mx start_97202563:

    # narrator2 "The girl couldn’t have been much older than Callia, yet she was missing a leg, the deviant burn having spread over nearly half her body."
    narrator2 "La niña no podía ser mucho mayor que Callia, pero le faltaba una pierna y la quemadura anormal se había esparcido por casi la mitad de su cuerpo."

# game/script.rpy:12498
translate spanish_mx start_b7d95ee8:

    # nu "I have a little girl just like her back home."
    nu "Tengo a una niña justo como ella en casa."

# game/script.rpy:12499
translate spanish_mx start_07da75de:

    # narrator2 "The nurse falls silent, her gaze locked straight ahead."
    narrator2 "La enfermera se queda callada, su mirada directamente hacia adelante."

# game/script.rpy:12500
translate spanish_mx start_92095ae3:

    # nu "…I hope she isn’t flagged."
    nu "…Espero que no sea marcada."

# game/script.rpy:12501
translate spanish_mx start_1c8acf17_20:

    # am "..."
    am "..."

# game/script.rpy:12502
translate spanish_mx start_0ae3d3c3:

    # narrator2 "“Flagged.”"
    narrator2 "“Marcada.”"

# game/script.rpy:12503
translate spanish_mx start_36dbe325:

    # narrator2 "A term that could only carry one meaning."
    narrator2 "Un término que solo podía significar una cosa."

# game/script.rpy:12504
translate spanish_mx start_c770ba6f:

    # narrator2 "It was as I thought."
    narrator2 "Era como lo pensé."

# game/script.rpy:12505
translate spanish_mx start_59c18f89:

    # narrator2 "The hospital was triaging its patients."
    narrator2 "El hospital triaba a sus pacientes."

# game/script.rpy:12506
translate spanish_mx start_e2e7107e:

    # narrator2 "My eyes drift down to the girl despite myself."
    narrator2 "Mis ojos bajan hacia la chica en contra de mi voluntad."

# game/script.rpy:12507
translate spanish_mx start_cfd3ee66:

    # narrator2 "Perhaps due to the nurse’s influence, I can’t help but see Callia’s image in the girl’s light brown locks and weakly shut eyes."
    narrator2 "Quizás por influencia de la enfermera, no puedo evitar ver la imagen de Callia en las trenzas castañas y ojos cerrados de la chica."

# game/script.rpy:12508
translate spanish_mx start_1ca0f8c1:

    # narrator2 "Everyone makes the same face when they’re struggling to hold onto life with all they have."
    narrator2 "Todos hacen el mismo rostro cuando se esfuerzan por mantenerse vivos con todo lo que tienen."

# game/script.rpy:12509
translate spanish_mx start_d169892f:

    # narrator2 "Though I want to avert my eyes…"
    narrator2 "Aunque intento apartar mis ojos…"

# game/script.rpy:12510
translate spanish_mx start_98f3ead5:

    # narrator2 "For some reason I feel compelled to look."
    narrator2 "Por alguna razón, me siento alentada a ver."

# game/script.rpy:12519
translate spanish_mx start_1565be38:

    # narrator2 "After rounding several corners, we enter a large open area where we’re directed to lay the stretchers down in a grid-like orientation."
    narrator2 "Tras dar vuelta en varias esquinas, entramos a una gran área abierta donde nos dirigen a dejar las camillas en el suelo en una disposición cuadricular."

# game/script.rpy:12520
translate spanish_mx start_8fc5023d:

    # narrator2 "Taking my place among the line of nurses gathering on one end of the room, I watch on as a pair of important looking people enters the area."
    narrator2 "Tomando lugar entre la línea de enfermeras reunidas en un borde del cuarto, veo cómo un par de personas de aspecto importante entra al área."

# game/script.rpy:12521
translate spanish_mx start_168dbc52:

    # chi "You’re certain there’s no other way director?"
    chi "¿Está segura de que no hay otra manera, directora?"

# game/script.rpy:12522
translate spanish_mx start_426b5119:

    # dir "There is no doubt."
    dir "No hay duda."

# game/script.rpy:12523
translate spanish_mx start_de746c20:

    # dir "It’s identical to the twelfth calamity that ravaged our homeland."
    dir "Es idéntica a la duodécima calamidad que arrasó con nuestra tierra natal."

# game/script.rpy:12524
translate spanish_mx start_66660916:

    # narrator2 "A silver-haired woman with an air of authority was discussing something with the chief of the Socotroni Guard."
    narrator2 "Una mujer de cabello plateado con un aire de autoridad discutía algo con el jefe de la Guardia Socotrina."

# game/script.rpy:12525
translate spanish_mx start_1aa6e49d:

    # chi "We can’t save more than half of the afflicted…?"
    chi "¿No podemos salvar a más de la mitad de los afectados…?"

# game/script.rpy:12526
translate spanish_mx start_aa1e3af2:

    # chi "There must be some kind of mistake."
    chi "Tiene que haber algún tipo de equivocación."

# game/script.rpy:12527
translate spanish_mx start_2a540c49:

    # chi "Surely there exists UL technology that can treat it."
    chi "Sin duda existe tecnología TD que puede tratarla."

# game/script.rpy:12528
translate spanish_mx start_7c6439b7:

    # dir "The deviant burn cannot be treated by medical science.{p=0.25}Not even amputation or debridement is effective."
    dir "La quemadura anormal no puede ser tratada por ciencia médica.{p=0.25}Ni siquiera las amputaciones o desbridamientos son efectivos."

# game/script.rpy:12529
translate spanish_mx start_240081b3:

    # dir "It is a death sentence that spurns the laws of the world."
    dir "Es una sentencia a muerte que rechaza las leyes del mundo."

# game/script.rpy:12530
translate spanish_mx start_38a9a1b1:

    # dir "That we are even able to save half is a fortune our homeland never knew."
    dir "El que seamos capaces de salvar a la mitad es una fortuna que nunca se conoció en nuestra tierra."

# game/script.rpy:12531
translate spanish_mx start_e3026790:

    # dir "Before we established this method the case fatality rate was 100 percent."
    dir "Antes de establecer este método, la tasa de casos fatales era del 100 porciento."

# game/script.rpy:12532
translate spanish_mx start_e766ead1:

    # chi "I see…"
    chi "Ya veo…"

# game/script.rpy:12533
translate spanish_mx start_507db6b7:

    # chi "I understand now why you were so quick to take the initiative."
    chi "Ahora entiendo por qué fue tan rápida en tomar la iniciativa."

# game/script.rpy:12534
translate spanish_mx start_9093b965:

    # narrator2 "A clipboard toting assistant runs up to the duo."
    narrator2 "Un asistente con un portapapeles se acerca corriendo al dúo."

# game/script.rpy:12535
translate spanish_mx start_fcc18477:

    # dir "Continue as planned."
    dir "Continúa conforme al plan."

# game/script.rpy:12536
translate spanish_mx start_62e60791:

    # narrator2 "The subordinate nods and dispatches a group of doctors to inspect the patients lying on the ground."
    narrator2 "El subordinado asiente y envía a un grupo de doctores a inspeccionar a los pacientes acostados en el suelo."

# game/script.rpy:12537
translate spanish_mx start_6682b1d1:

    # narrator2 "One by one, the doctors examine the patients before attaching colored tags to their stretcher."
    narrator2 "Uno por uno, los doctores examinan a los pacientes antes de colocar etiquetas de colores en sus camillas."

# game/script.rpy:12538
translate spanish_mx start_78ae72a3:

    # narrator2 "Black.{w=0.25} Red."
    narrator2 "Negro.{w=0.25} Rojo."

# game/script.rpy:12539
translate spanish_mx start_d3f821b0:

    # narrator2 "Black.{w=0.2} Black.{w=0.25} Red."
    narrator2 "Negro.{w=0.2} Negro.{w=0.25} Rojo."

# game/script.rpy:12540
translate spanish_mx start_2cd74a83:

    # chi "There remains a problem though."
    chi "Pero aún queda un problema."

# game/script.rpy:12541
translate spanish_mx start_deeb0e64:

    # chi "What do we tell the people?"
    chi "¿Qué le decimos a la gente?"

# game/script.rpy:12542
translate spanish_mx start_ca5a3b50:

    # chi "They are beleaguered and battered, and their frustration continues to grow by the hour."
    chi "Están atormentados y abatidos, y su frustración continúa creciendo a cada minuto."

# game/script.rpy:12543
translate spanish_mx start_e6f0fbbb:

    # chi "We may see violent protest if we continue to keep them in the dark."
    chi "Podríamos ver una protesta violenta si los seguimos dejando en la ignorancia."

# game/script.rpy:12544
translate spanish_mx start_3a692e13:

    # dir "We alone will shoulder the truth."
    dir "Nosotros asumiremos la verdad."

# game/script.rpy:12545
translate spanish_mx start_ab9b5cb5:

    # dir "If a riot occurs, you have clearance to suppress it through any means necessary."
    dir "Si se arma un disturbio, tienes permiso de suprimirlo mediante cualquier medio que sea necesario."

# game/script.rpy:12546
translate spanish_mx start_bb47a69b:

    # chi "Nearly our entire force has been mobilized here, but I fear even that may not be enough."
    chi "Casi toda nuestra fuerza ha sido movilizada aquí, pero temo que incluso eso no sea suficiente."

# game/script.rpy:12547
translate spanish_mx start_d3256782:

    # dir "There is no need for alarm."
    dir "No hay necesidad de alarmarse."

# game/script.rpy:12548
translate spanish_mx start_eae8af8b:

    # dir "It is a last resort since they are poorly suited for nonlethal mass suppression, but the Lost Children are stationed here as well."
    dir "Es un último recurso, ya que son poco adecuados para la supresión masiva no letal, pero los Niños Perdidos están estacionados aquí también."

# game/script.rpy:12549
translate spanish_mx start_4b0669bf:

    # dir "They are currently carrying out patient acquisition operations at the moment."
    dir "Actualmente están llevando a cabo operaciones de adquisición de pacientes."

# game/script.rpy:12550
translate spanish_mx start_184b852c:

    # chi "You even have a system for abducting noncompliant victims, huh…"
    chi "Incluso tiene un sistema para abducir a las víctimas inconformes, eh…"

# game/script.rpy:12551
translate spanish_mx start_7a45e096:

    # dir "For the sake of saving as many possible, there is no cross we will not bear."
    dir "Por el bien de salvar a tantos como sea posible, no hay cruz que no podamos soportar."

# game/script.rpy:12552
translate spanish_mx start_06cd28e5:

    # dir "We will go to any length towards that end."
    dir "Usaremos cualquier medio para ese fin."

# game/script.rpy:12553
translate spanish_mx start_96b20e0f:

    # dir "That is the burden of surviving what we did back in our homeland."
    dir "Esa es la carga de haber sobrevivido lo que pasó en nuestra tierra natal."

# game/script.rpy:12554
translate spanish_mx start_b05cdd79:

    # chi "…If this is the path we must take to save as many as we can, that is what we will do."
    chi "…Si este es el camino que debemos tomar para salvar a todos los que podamos, entonces eso será lo que haremos."

# game/script.rpy:12555
translate spanish_mx start_539316a4:

    # chi "The Socotroni Guard will ensure operations in Aachen Neues are not hindered."
    chi "La Guardia Socotrina se asegurará de que las operaciones en Aachen Neues no se vean obstaculizadas."

# game/script.rpy:12556
translate spanish_mx start_aea82a5e:

    # dir "..."
    dir "..."

# game/script.rpy:12557
translate spanish_mx start_3e4c3503:

    # dir "A wounded people, scattered by the upheaval of sudden calamity."
    dir "Un pueblo herido, disperso por el disturbio de una calamidad repentina."

# game/script.rpy:12558
translate spanish_mx start_91fae6f5:

    # dir "When forced to confront the reality of their helplessness, they will search for release."
    dir "Al ser forzados a enfrentar la realidad de su desesperación, buscarán la libertad."

# game/script.rpy:12559
translate spanish_mx start_066870d8:

    # dir "Something.{w=0.25} Anything, that gives them a means to resist their powerlessness."
    dir "Algo.{w=0.25} Lo que sea, que les de un medio para resistir su impotencia."

# game/script.rpy:12560
translate spanish_mx start_d16fb2e8:

    # dir "It is a fire that cannot be doused."
    dir "Es un fuego que no puede extinguirse."

# game/script.rpy:12561
translate spanish_mx start_469ce2e6:

    # dir "Should it be forbidden from challenging the source of its frustration, its flames will simply aggress in another direction and consume something else."
    dir "Si se le prohibiera confrontar la fuente de su frustración, sus llamas simplemente agredirían hacia otra dirección y consumirían algo más."

# game/script.rpy:12562
translate spanish_mx start_849b9b74:

    # chi "You can’t mean…"
    chi "No querrá decir…"

# game/script.rpy:12563
translate spanish_mx start_4e748526:

    # dir "A scapegoat."
    dir "Un chivo expiatorio."

# game/script.rpy:12564
translate spanish_mx start_546dd080:

    # dir "It happened often back in our homeland."
    dir "Sucedía con frecuencia en nuestra tierra."

# game/script.rpy:12565
translate spanish_mx start_a1e4bd20:

    # dir "If a scapegoat is created…{w=0.4} do not try to save them."
    dir "Si se crea un chivo expiatorio…{w=0.4} no intenten salvarlo."

# game/script.rpy:12566
translate spanish_mx start_dc368b74:

    # chi "..."
    chi "..."

# game/script.rpy:12567
translate spanish_mx start_07aea1fe:

    # dir "We made that mistake in the past…{w=0.35} it is not something that bears repeating in Socotrine."
    dir "Cometimos ese error en el pasado…{w=0.35} no es algo que soportaríamos repetir en Socotrine."

# game/script.rpy:12568
translate spanish_mx start_aad35ec4:

    # dir "Let them have their scapegoat.{p=0.25}That is how deep the pus runs in the people’s hearts."
    dir "Permíteles tener a su chivo expiatorio.{p=0.25}Así de profundo corre la pus en los corazones de la gente."

# game/script.rpy:12569
translate spanish_mx start_45b951a4:

    # dir "Do you understand?"
    dir "¿Entiendes?"

# game/script.rpy:12570
translate spanish_mx start_73b232a2:

    # chi "Y-Yes…"
    chi "S-Sí…"

# game/script.rpy:12571
translate spanish_mx start_62641425:

    # narrator2 "Having finished her business with him, the director proceeds deeper into the building."
    narrator2 "Al haber terminado sus asuntos con él, la directora procede hacia lo profundo del edificio."

# game/script.rpy:12572
translate spanish_mx start_1e841806:

    # chi "Haven’t we had enough sacrifices already…?"
    chi "¿No hemos tenido ya suficientes sacrificios…?"

# game/script.rpy:12573
translate spanish_mx start_a6a0f8fd:

    # narrator2 "Hunching over wearily, the chief departs from the room."
    narrator2 "Encorvándose cansadamente, el jefe sale del cuarto."

# game/script.rpy:12574
translate spanish_mx start_5572ea08:

    # narrator2 "Before long, the doctors finish their examinations and the head nurse signals us forward."
    narrator2 "Tras poco tiempo, los doctores terminan sus examinaciones y la jefa enfermera nos señala hacia adelante."

# game/script.rpy:12575
translate spanish_mx start_9949fa78:

    # narrator2 "I find myself gravitating towards my previous patient where I find the same nurse from before."
    narrator2 "Me encuentro gravitando hacia mi paciente anterior, donde encuentro a la misma enfermera de antes."

# game/script.rpy:12579
translate spanish_mx start_4da31d53:

    # narrator2 "Lifting the stretcher up, I begin following a stream of nurses only to stop short from my partner’s resistance." with Shake((0, 0, 0, 0), 0.2, dist=3)
    narrator2 "Levantando la camilla, comienzo a seguir el flujo de enfermeras solo para detenerme en mi lugar por la resistencia de mi compañera." with Shake((0, 0, 0, 0), 0.2, dist=3)

# game/script.rpy:12580
translate spanish_mx start_f51c9af1:

    # nu "..."
    nu "..."

# game/script.rpy:12581
translate spanish_mx start_29006609:

    # nu "Black doesn’t go this way."
    nu "Negro no va por aquí."

# game/script.rpy:12582
translate spanish_mx start_b1d722af:

    # narrator2 "Letting her take point, we join a different procession of nurses heading deeper in."
    narrator2 "Dejándola guiar el camino, nos unimos a una procesión de enfermeras diferente dirigiéndose aún más profundo."

# game/script.rpy:12583
translate spanish_mx start_c4bd94cd:

    # nu "The ones with missing limbs are always flagged after all…"
    nu "Después de todo, siempre marcan a los que les faltan extremidades…"

# game/script.rpy:12584
translate spanish_mx start_e9083af6:

    # nu "Hah…{w=0.3} even though I knew that…"
    nu "Jah…{w=0.3} aunque ya lo sabía…"

# game/script.rpy:12593
translate spanish_mx start_3d812a7d:

    # narrator2 "Several ramps later we arrive at a metal underground room divided by a partition."
    narrator2 "Varias rampas después llegamos a un cuarto subterráneo de metal dividido por una partición." 

# game/script.rpy:12594
translate spanish_mx start_48a3ca17:

    # narrator2 "There, we’re directed to lift and place the patients on the other side."
    narrator2 "Ahí, nos dirigen para levantar y colocar a los pacientes del otro lado."

# game/script.rpy:12595
translate spanish_mx start_5ae96ff2:

    # narrator2 "Upon completing our task the majority of the nurses start to leave."
    narrator2 "Tras completar la tarea la mayoría de enfermeras empieza a irse."

# game/script.rpy:12596
translate spanish_mx start_5ceec032:

    # narrator2 "However, my partner doesn’t budge from her spot."
    narrator2 "Sin embargo, mi compañera no se mueve de su lugar."

# game/script.rpy:12597
translate spanish_mx start_e8797658:

    # narrator2 "A departing nurse places a hand on her shoulder."
    narrator2 "Una enfermera saliente pone una mano sobre su hombro."

# game/script.rpy:12598
translate spanish_mx start_477e33ad:

    # nu2 "You don’t have to watch."
    nu2 "No tienes que verlo."

# game/script.rpy:12599
translate spanish_mx start_27574bd1:

    # narrator2 "But my partner shakes her head."
    narrator2 "Pero mi compañera sacude su cabeza."

# game/script.rpy:12600
translate spanish_mx start_1924e22d:

    # nu "I don’t want to look away from what we’re doing."
    nu "No quiero apartar la vista de lo que estamos haciendo."

# game/script.rpy:12601
translate spanish_mx start_401b3e44:

    # narrator2 "When preparations are complete a woman with steely gray eyes enters the room."
    narrator2 "Cuando los preparativos están listos, una mujer de férreos ojos grises entra a la habitación."

# game/script.rpy:12602
translate spanish_mx start_83835c93:

    # narrator2 "It was the director from before, accompanied by her clipboard-toting shadow."
    narrator2 "Era la directora de antes, acompañada por su asistente del portapapeles." 

# game/script.rpy:12603
translate spanish_mx start_fb4aad96:

    # dir "Begin."
    dir "Empiecen."

# game/script.rpy:12607
translate spanish_mx start_eda1696d:

    # narrator2 "At her command a click resounds through the room."
    narrator2 "A su orden, un chasquido resuena por toda la habitación."

# game/script.rpy:12611
translate spanish_mx start_caa97c4e:

    # narrator2 "A deep rumbling begins to fill the air."
    narrator2 "Un profundo retumbar comienza a llenar el aire."

# game/script.rpy:12612
translate spanish_mx start_ad2e08f6:

    # narrator2 "Beyond the glass the air starts shimmering as several sections of the steel wall flare up."
    narrator2 "Tras el cristal, el aire empieza a resplandecer mientras varias secciones de la pared de acero se incendian."

# game/script.rpy:12617
translate spanish_mx start_595e21c1:

    # narrator2 "The room was… heating up."
    narrator2 "El cuarto estaba… calentándose."

# game/script.rpy:12618
translate spanish_mx start_6fc64a62:

    # narrator2 "Did they intend to cook the patients alive…?"
    narrator2 "¿Pretendían cocinar a los pacientes vivos…?"

# game/script.rpy:12619
translate spanish_mx start_22bf9d00:

    # narrator2 "Unable to do anything but look on, my eyes drift over to the girl I had carried here."
    narrator2 "Incapaz de hacer nada más que seguir observando, mis ojos se desplazan hacia la niña que había cargado hasta aquí."

# game/script.rpy:12620
translate spanish_mx start_3bf7d778:

    # narrator2 "That’s when I notice her beginning to stir."
    narrator2 "Ahí es cuando noto que comienza a agitarse."

# game/script.rpy:12624
translate spanish_mx start_6945e848:

    # nu3 "S-Somebody’s moving in there!" with Shake((0, 0, 0, 0), 0.5, dist=10)
    nu3 "¡A-Alguien se está moviendo adentro!" with Shake((0, 0, 0, 0), 0.5, dist=10)

# game/script.rpy:12625
translate spanish_mx start_484bc2e1:

    # narrator2 "Someone cries out in horror as they notice as well."
    narrator2 "Alguien grita del horror al percatarse también."

# game/script.rpy:12626
translate spanish_mx start_51966ce6:

    # narrator2 "The girl sits up."
    narrator2 "La niña se sienta."

# game/script.rpy:12627
translate spanish_mx start_2d42a5ae:

    # narrator2 "She looks at her hands."
    narrator2 "Ve sus manos."

# game/script.rpy:12628
translate spanish_mx start_e7e7b780:

    # narrator2 "Then, she begins to scream."
    narrator2 "Entonces, comienza a gritar."

# game/script.rpy:12629
translate spanish_mx start_972ec850:

    # narrator2 "Loudly." with Shake((0, 0, 0, 0), 1.5, dist=13)
    narrator2 "Ruidosamente." with Shake((0, 0, 0, 0), 1.5, dist=13)

# game/script.rpy:12630
translate spanish_mx start_c3af3b36:

    # narrator2 "At the top of her lungs." with Shake((0, 0, 0, 0), 2.0, dist=17)
    narrator2 "A todo pulmón." with Shake((0, 0, 0, 0), 2.0, dist=17)

# game/script.rpy:12631
translate spanish_mx start_62e12793:

    # narrator2 "As the intense heat envelops her, the deviant burns on her hands begin glowing brightly before surging up her body at an accelerated rate."
    narrator2 "Mientras el calor intenso la envuelve, las quemaduras anormales en sus manos comienzan a brillar vivamente antes de cubrir su cuerpo a un ritmo acelerado."

# game/script.rpy:12635
translate spanish_mx start_52cfdfeb:

    # narrator2 "Shocked silence grips the room as the girl collapses and begins weakly crawling along the floor." with Shake((0, 0, 0, 0), 0.5, dist=4)
    narrator2 "Un silencio estupefacto se apodera del cuarto mientras la niña colapsa y comienza a arrastrarse débilmente por el piso." with Shake((0, 0, 0, 0), 0.5, dist=4)

# game/script.rpy:12636
translate spanish_mx start_c97811d2:

    # narrator2 "She doesn’t make it far before her body is consumed and she crumbles into glittering dust, but she continues screaming until the very last moment." with Shake((0, 0, 0, 0), 0.7, dist=7)
    narrator2 "No llega muy lejos antes de que su cuerpo es consumido y se desmorona en un polvo brillante, pero continúa gritando hasta el último momento." with Shake((0, 0, 0, 0), 0.7, dist=7)

# game/script.rpy:12640
translate spanish_mx start_7a19e74f:

    # narrator2 "At my side I hear a hollow thump as my partner falls to her knees." with Shake((0, 0, 0, 0), 0.5, dist=10)
    narrator2 "A mi lado escucho un golpe hueco mientras mi compañera cae sobre sus rodillas." with Shake((0, 0, 0, 0), 0.5, dist=10)

# game/script.rpy:12644
translate spanish_mx start_e61cf511:

    # nu "No…"
    nu "No…"

# game/script.rpy:12645
translate spanish_mx start_809c31c5:

    # nu "I couldn’t have botched the injection, could I...?"
    nu "No pude haber fallado con la inyección, ¿cierto...?"

# game/script.rpy:12646
translate spanish_mx start_07c76e49:

    # narrator2 "It was as if the room had been traumatized into silence."
    narrator2 "Era como si el cuarto se hubiese quedado callado del trauma."

# game/script.rpy:12650
translate spanish_mx start_b78782b0:

    # narrator2 "At long last, the rumbling stops."
    narrator2 "Finalmente, el retumbar se detiene."

# game/script.rpy:12655
translate spanish_mx start_b3aede93:

    # narrator2 "A hissing sound fills the air as the chamber begins venting."
    narrator2 "Un sonido siseante llena el aire mientras la cámara se empieza a ventilar."

# game/script.rpy:12656
translate spanish_mx start_d8373e3b:

    # nu3 "Director…{w=0.3} why can’t we terminate the patients before doing this?"
    nu3 "Directora…{w=0.3} ¿por qué no podemos liquidar a los pacientes antes de hacer esto?" ## lmao

# game/script.rpy:12657
translate spanish_mx start_aea82a5e_1:

    # dir "..."
    dir "..."

# game/script.rpy:12658
translate spanish_mx start_dbfca7d4:

    # dir "The deviant burn does not spread on cadavers."
    dir "La quemadura anormal no se esparce en cadáveres."

# game/script.rpy:12659
translate spanish_mx start_c829217d:

    # dir "Only when it finishes consuming a living subject is Estus Vitae produced."
    dir "Solo es cuando termina de consumir a un sujeto vivo que se produce Estus Vitae."

# game/script.rpy:12660
translate spanish_mx start_11949dfa:

    # nu "I can’t do this anymore..."
    nu "Ya no puedo seguir con esto..."

# game/script.rpy:12661
translate spanish_mx start_2823313a:

    # nu "Please...{w=0.3} no more…"
    nu "Por favor...{w=0.3} ya no más…"

# game/script.rpy:12662
translate spanish_mx start_e2723cbe:

    # dir "We are already short on staff."
    dir "Ya estamos cortos de personal."

# game/script.rpy:12663
translate spanish_mx start_7039abbe:

    # dir "Even with everyone working around the clock, there are many that are lost due to our sheer lack of manpower."
    dir "Incluso con todos trabajando día y noche, hay muchas pérdidas debido a nuestra simple falta de mano de obra."

# game/script.rpy:12664
translate spanish_mx start_56eab9d8:

    # dir "You know this already."
    dir "Ya deberían saber esto."

# game/script.rpy:12665
translate spanish_mx start_f51c9af1_1:

    # nu "..."
    nu "..."

# game/script.rpy:12666
translate spanish_mx start_75b4b427:

    # dir "This is the burden those in this post must bear."
    dir "Esta es una carga que aquellos en este puesto deben soportar."

# game/script.rpy:12667
translate spanish_mx start_196d67c8:

    # dir "From now on, ensure you follow protocol and adjust the dosage properly for pediatric patients."
    dir "Desde ahora, asegúrense de seguir el protocolo y ajustar la dosis adecuadamente para los pacientes pediátricos."

# game/script.rpy:12668
translate spanish_mx start_1c51395b:

    # narrator2 "Nothing remained of the people beyond the glass but vaguely shaped piles of glittering dust."
    narrator2 "Nada quedaba de la gente tras el cristal más que montones vagamente formados de polvo brillante."

# game/script.rpy:12669
translate spanish_mx start_dcd59d5a:

    # narrator2 "Unfairly, and without warning, their struggle had ended."
    narrator2 "Injustamente, y sin previo aviso, su dolor había terminado."

# game/script.rpy:12670
translate spanish_mx start_8fc2ad71:

    # narrator2 "I watch on as the dust swirls and coalesces into tiny orbs— one for each person that had been inside."
    narrator2 "Observo cómo el polvo gira y converge en pequeños orbes— uno por cada persona que había estado dentro."

# game/script.rpy:12671
translate spanish_mx start_65fd293e:

    # narrator2 "When the room finishes cooling, we’re directed to enter and collect them."
    narrator2 "Cuando la habitación termina de enfriarse, se nos dirige a entrar y recolectarlos."

# game/script.rpy:12672
translate spanish_mx start_95a65bbb:

    # narrator2 "The remains of the girl I had carried here."
    narrator2 "Los restos de la niña que había llevado hasta aquí."

# game/script.rpy:12673
translate spanish_mx start_1860ffe8:

    # narrator2 "I pause before them and kneel down."
    narrator2 "Me detengo frente a ellos y me arrodillo."

# game/script.rpy:12677
translate spanish_mx start_c4bd94cd_1:

    # nu "The ones with missing limbs are always flagged after all…"
    nu "Después de todo, siempre marcan a los que les faltan extremidades…"

# game/script.rpy:12681
translate spanish_mx start_deda5908:

    # narrator2 "If I bring Callia here…{w=0.35} this is what will become of her."
    narrator2 "Si traigo a Callia aquí…{w=0.35} esto es en lo que se convertirá."

# game/script.rpy:12682
translate spanish_mx start_0d84811b:

    # narrator2 "This is how she will end up."
    narrator2 "Esto es lo que será de ella."

# game/script.rpy:12683
translate spanish_mx start_e50bff29:

    # narrator2 "The others as well.{w=0.25} There was no guarantee they wouldn’t be flagged."
    narrator2 "Los demás también.{w=0.25} Nada garantizaba que no serían marcados."

# game/script.rpy:12684
translate spanish_mx start_1c8acf17_21:

    # am "..."
